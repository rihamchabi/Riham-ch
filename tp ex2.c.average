#include <stdio.h>
float average(int arr[],int n){
float sum=0;
for(int i=0;i<n;i++){
sum+=arr[i];
return sum/n;
}
}
int main() {
int size;
float arr[size];
printf("donner size");
scanf("%d",size);
for(int i=0;i<size;i++){
scanf("%f",&arr[i]);
printf("the average=%f",average(arr,size));
return 0; 
}
